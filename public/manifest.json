{
    "manifest_version": 3,
    "name": "DraftEase",
    "version": "1.0",
    "description": "Transfer ChatGPT content directly to WordPress.",
    "permissions": ["activeTab", "storage", "clipboardRead"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "index.html"
    }
  }
  